{
  "name": "@metamask/ethereum-provider-openrpc-generator",
  "version": "1.0.34",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/ethereum-provider-openrpc-generator.git"
  },
  "main": "./build/index.js",
  "files": [
    "build/**/*",
    ".node-version"
  ],
  "scripts": {
    "start": "yarn build && open-rpc-generator generate -c open-rpc-generator-config.json",
    "test": "yarn test:lint && npm run test:unit",
    "test:unit": "jest --coverage --passWithNoTests",
    "test:integration": "yarn build && open-rpc-generator generate -c test-generator-config.json",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' '!.yarnrc.yml' --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "build": "yarn build:clean && tsc && cp -r templates build/",
    "build:clean": "rm -rf build",
    "watch:build": "tsc --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@etclabscore/json-schema-to-types": "^1.10.1",
    "@open-rpc/generator": "^1.21.2",
    "@open-rpc/typings": "^1.8.0",
    "fs-extra": "7.0.1",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.1",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@open-rpc/meta-schema": "^1.6.0",
    "@types/fs-extra": "8.0.1",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.172",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-it-up": "^2.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.7.0",
    "typedoc": "^0.23.15",
    "typescript": "~4.8.4"
  }
}
